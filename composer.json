{
    "name": "compwright/aws-env",
    "type": "library",
    "description": "Load environment variables securely from AWS SSM",
    "license": "MIT",
    "authors": [
        {
            "name": "Jonathon Hill",
            "email": "jonathon@compwright.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "Compwright\\AwsEnv\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
